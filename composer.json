{
  "name": "gravity-pdf/developer-toolkit",
  "type": "project",
  "license": "GPLv2",
  "require": {
    "php": ">=5.4"
  },
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "GravityPDFDeveloperToolkitAddon"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.0 <7"
  },
  "autoload": {
    "psr-4": {
      "GFPDF\\Plugins\\DeveloperToolkit\\": "src/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "yarn global add gulp-cli",
      "yarn && gulp"
    ]
  }
}
